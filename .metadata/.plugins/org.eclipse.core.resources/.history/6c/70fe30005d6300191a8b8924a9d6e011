    function chknull() {
        if (document.getElementById('enumber').value == '') {
            alert('工号不能为空');
            form1.GongHao.focus();
            return false;
        }
        if (document.getElementById('ename').value == '') {
            alert('姓名不能为空');
            form1.XingMing.focus();
            return false;
        }
        if (document.getElementById('dateofentry').value == '') {
            alert('入职日期不能为空');
            form1.DaoGangRQ.focus();
            return false;
        }
        if (document.getElementById('edepartmentnum').value == '') {
            alert('部门名称不能为空');
            form1.YinPinBM.focus();
            return false;
        }
        if (document.getElementById('basicwage').value == '') {
            alert('基本工资不能为空');
            form1.JbGongZ.focus();
            return false;
        }
        if (document.getElementById('basicwage').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('basicwage').value)) {
                alert("基本工资只能为正数");
                form1.JbGongZ.focus();
                return false;
            }
        }
        if (document.getElementById('gwgz').value == '') {
            alert('岗位工资不能为空');
            form1.Gwgz.focus();
            return false;
        }
        if (document.getElementById('gwgz').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('gwgz').value)) {
                alert("岗位工资只能为正数");
                form1.Gwgz.focus();
                return false;
            }
        }
        if (document.getElementById('jxgz').value == '') {
            alert('绩效工资不能为空');
            form1.Jxgz.focus();
            return false;
        }
        if (document.getElementById('jxgz').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('jxgz').value)) {
                alert("绩效工资只能为正数");
                form1.Jxgz.focus();
                return false;
            }
        }
        if (document.getElementById('glgz').value == '') {
            alert('工龄工资不能为空');
            form1.Glgz.focus();
            return false;
        }
        if (document.getElementById('glgz').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('glgz').value)) {
                alert("工龄工资只能为0或正数");
                form1.Glgz.focus();
                return false;
            }
        }
        if (document.getElementById('gwjt').value == '') {
            alert('岗位津贴不能为空');
            form1.Gwjt.focus();
            return false;
        }
        if (document.getElementById('gwjt').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('gwjt').value)) {
                alert("岗位津贴只能为0或正数");
                form1.Gwjt.focus();
                return false;
            }
        }
        if (document.getElementById('hfbt').value == '') {
            alert('话费补贴不能为空');
            form1.Hfbt.focus();
            return false;
        }
        if (document.getElementById('hfbt').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('hfbt').value)) {
                alert("话费补贴只能为正数");
                form1.Hfbt.focus();
                return false;
            }
        }
        if (document.getElementById('cfbt').value == '') {
            alert('餐费补贴不能为空');
            form1.Cfbt.focus();
            return false;
        }
        if (document.getElementById('cfbt').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('cfbt').value)) {
                alert("餐费补贴只能为正数");
                form1.Cfbt.focus();
                return false;
            }
        }
        if (document.getElementById('jtbt').value == '') {
            alert('交通补贴不能为空');
            form1.Jtbt.focus();
            return false;
        }
        if (document.getElementById('jtbt').value != '') {
            var objRe = /^\d+(\.\d+)?$/;
            if (!objRe.test(document.getElementById('jtbt').value)) {
                alert("交通补贴只能为正数");
